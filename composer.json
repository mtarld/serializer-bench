{
    "name": "soyuka/serializer-benchmarks",
    "require-dev": {
        "phpbench/phpbench": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Soyuka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soyuka\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../marshaller/components/Marshaller"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "api-platform/core": "^3.0",
        "symfony/marshaller": "*"
    }
}
